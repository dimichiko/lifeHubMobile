{
  "name": "lifehub-pnpm",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Tests run in individual apps\" && exit 0",
    "db:seed": "npx tsx prisma/seed.ts",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.12.4",
  "devDependencies": {
    "@nestjs/cli": "^11.0.7",
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^24.0.10",
    "bcrypt": "^6.0.0",
    "eslint": "^9.30.1",
    "husky": "^8.0.0",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "prisma": "^6.11.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "turbo": "^2.5.4",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@prisma/client": "^6.11.1",
    "zod": "^4.0.5"
  },
  "pnpm": {
    "overrides": {
      "use-latest-callback": "0.1.5"
    },
    "ignoredBuiltDependencies": [
      "@swc/core"
    ],
    "onlyBuiltDependencies": [
      "@nestjs/core",
      "@prisma/client",
      "@prisma/engines",
      "bcrypt",
      "prisma"
    ]
  }
}
